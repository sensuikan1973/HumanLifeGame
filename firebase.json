{
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "/__/firebase/init.json",
        "headers": [
          {
            "key" : "Access-Control-Allow-Origin",
            "value" : "http://localhost:8686"
          }
        ]
      }
    ]
  }
}
